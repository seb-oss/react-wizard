{
  "name": "@sebgroup/react-wizard",
  "version": "0.0.0-semantically-released",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "homepage": "https://sebgroup.github.io/react-wizard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebgroup/react-wizard.git"
  },
  "bugs": {
    "url": "https://github.com/sebgroup/react-wizard/issues"
  },
  "license": "Apache License 2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "react",
    "components",
    "javascript",
    "typescript",
    "frontend",
    "ui",
    "wizard"
  ],
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "pupa": "^2.1.1",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0",
    "postcss": "^8.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.35.1",
    "semantic-release": "^17.4.4",
    "style-loader": "^1.3.0",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.2"
  },
  "peerDependencies": {
    "@sebgroup/bootstrap": "^5.4.1",
    "@sebgroup/react-components": "^5.0.0-beta.15",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "lint": "eslint --ext=.js,.jsx,.ts,.tsx --ignore-path=.gitignore .",
    "build:docs": "build-storybook",
    "prebuild:lib": "rimraf dist/* && node scripts/indexer.js",
    "build:lib": "rollup -c",
    "postbuild:lib": "node scripts/postBuild.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d storybook-static",
    "commit": "cz",
    "release": "semantic-release",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.(j|t)s?(x)",
      "!src/**/*.d.ts",
      "!src/**/index.(j|t)s?(x)",
      "!src/**/*.stories.(j|t)s?(x)",
      "!src/index.(j|t)s?(x)",
      "!src/reportWebVitals.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(react|@sebgroup/react-components|@sebgroup/frontend-tools)/)"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
